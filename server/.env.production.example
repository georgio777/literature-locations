# PRODUCTION SERVER ENVIRONMENT VARIABLES
# ВАЖНО: Замените все значения на реальные продакшен данные!

# Database connection для продакшена
DATABASE_URL=postgresql://your_db_user:your_secure_password@postgres:5432/your_database_name

# JWT Secret (ОБЯЗАТЕЛЬНО сгенерируйте новый для продакшена!)
JWT_SECRET=your_super_secure_jwt_secret_minimum_64_characters

# Admin credentials для продакшена
ADMIN_LOGIN=your_admin_username
ADMIN_PASSWORD=your_very_secure_admin_password

# Server port
PORT=3001

# Дополнительные настройки для продакшена
NODE_ENV=production