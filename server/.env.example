# Database connection (замените на свои данные)
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# JWT Secret (сгенерируйте случайную строку длиной 32+ символа)
JWT_SECRET=your_super_secret_jwt_key_here_32_chars_minimum

# Admin credentials (установите свои учетные данные)
ADMIN_LOGIN=your_admin_login
ADMIN_PASSWORD=your_secure_admin_password

# Server port
PORT=3001