# PRODUCTION SERVER ENVIRONMENT VARIABLES
# ВАЖНО: Замените все значения на реальные продакшен данные!

# Database connection для продакшена
DATABASE_URL=postgresql://prod_user:SECURE_PROD_PASSWORD@your_prod_db_host:5432/literature_locations_prod

# JWT Secret (ОБЯЗАТЕЛЬНО сгенерируйте новый для продакшена!)
JWT_SECRET=PRODUCTION_JWT_SECRET_MINIMUM_64_CHARACTERS_VERY_SECURE_STRING

# Admin credentials для продакшена
ADMIN_LOGIN=your_production_admin_username
ADMIN_PASSWORD=VERY_SECURE_PRODUCTION_ADMIN_PASSWORD

# Server port
PORT=3001

# Дополнительные настройки для продакшена
NODE_ENV=production